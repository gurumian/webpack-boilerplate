{
  "name": "webpack-boilerplate",
  "version": "0.0.1",
  "description": "simple player using threejs",
  "scripts": {
    "dev:server": "webpack serve --config ./src/server/webpack.dev.js",
    "build:server": "webpack --config ./src/server/webpack.prod.js",
    "build:client": "webpack --config ./src/client/webpack.prod.js",
    "build": "npm run build:client && npm run build:server",
    "dev": "webpack serve --config ./src/client/webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cd dist/server && node ./server.js",
    "pack": "snapcraft --use-lxd"
  },
  "author": "Bryce Ghim",
  "license": "ISC",
  "devDependencies": {
    "@types/dat.gui": "^0.7.13",
    "@types/jsmediatags": "^3.9.6",
    "@types/three": "^0.169.0",
    "@types/uuid": "^8.3.4",
    "copy-webpack-plugin": "^12.0.2",
    "dat.gui": "^0.7.9",
    "nodemon": "^3.1.7",
    "path-browserify": "^1.0.1",
    "three": "^0.169.0",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "jsmediatags": "^3.9.7",
    "long-press-event": "^2.5.0",
    "process": "^0.11.10",
    "uuid": "^10.0.0"
  }
}
